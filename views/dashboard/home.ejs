<script>
  $(document).ready(function() {
    setTimeout(() => {
      const storedItem = localStorage.getItem('activeItem');
      const timestamp = localStorage.getItem('activeItemTimestamp');

      if (storedItem !== null) {
        // Check if the stored item is not expired (2 minutes)
        if (storedItem && timestamp && Date.now() - timestamp < 100 * 60 * 1000) {
          // Trigger click event on the corresponding list item
          $(`#_${storedItem}`).trigger('click');
        }
      }
    }, 1000);
  })
</script>