<script src="../js/module/global.js" defer></script>
<script src="../js/loading.js" defer></script>
<script>
    function plan2(){

      // Firsttime payment on paystack
      async function startPaystack(){
        let settings = {
          method: 'GET',
          headers: {
            "Authorization": `Bearer ${localStorage.getItem('access')}`,
          }
        };
       
        
        const myButton = document.getElementById('plan2');
        // Call losding function
        start_loading(myButton)

        try {
          let pay = await fetch(`http://localhost:5000/api/pay`, settings);
          let status = await pay.status
          let json = await pay.json();
          console.log(json)
          if (status == 200) {
            window.location.href = json.data
          }
        }catch(err){
          console.log(err)
        }finally{
          setTimeout(function () {
            end_loading(myButton)
           }, 1000)
        }
      }
      startPaystack()
    }
</script>